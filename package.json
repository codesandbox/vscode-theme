{
  "name": "codesandbox-theme",
  "description": "A CodeSandbox theme for Visual Studio Code",
  "version": "1.0.0",
  "publisher": "CodeSandbox-io",
  "scripts": {
    "release": "yes | vsce publish $BUMP --no-git-tag-version"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/odesandbox/vscode-theme"
  },
  "author": {
    "name": "CodeSandbox",
    "url": "https://codesandbox.io"
  },
  "icon": "./media/icon.png",
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "CodeSandbox",
        "uiTheme": "vs-dark",
        "path": "./themes/codesandbox-dark.json"
      }
    ]
  },
  "engines": {
    "vscode": "^1.22.0"
  },
  "galleryBanner": {
    "color": "#000000",
    "theme": "dark"
  },
  "devDependencies": {
    "vsce": "^2.7.0"
  }
}
